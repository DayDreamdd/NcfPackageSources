<Project Sdk="Microsoft.NET.Sdk.Razor">
  <PropertyGroup>
    <TargetFramework>net8.0</TargetFramework>
    <Version>0.1.0</Version>
    <AssemblyName>Senparc.Xncf.AIAgentsHub</AssemblyName>
    <RootNamespace>Senparc.Xncf.AIAgentsHub</RootNamespace>
    <GenerateEmbeddedFilesManifest>true</GenerateEmbeddedFilesManifest>
    <AddRazorSupportForMvc>true</AddRazorSupportForMvc>
    <GeneratePackageOnBuild Condition=" '$(Configuration)' == 'Release' ">true</GeneratePackageOnBuild>
    <Description>人工智能代理枢纽模块</Description>
    <Copyright>Senparc</Copyright>
    <PackageTags>Senparc,NeuCharFramework,NCF,Senparc.Xncf.AIAgentsHub</PackageTags>
    <Authors>Senparc</Authors>
    <Owners>Senparc</Owners>
    <PackageLicenseExpression>Apache-2.0</PackageLicenseExpression>
    <Title>AI Agent Hub</Title>
    <!--<ProjectUrl> https://github.com/NeuCharFramework/NCF</ProjectUrl>
    <PackageProjectUrl>https://github.com/NeuCharFramework/NcfPackageSources</PackageProjectUrl>-->
    <PackageIcon>icon.jpg</PackageIcon>
    <PackageReleaseNotes>
			v0.1.0 创世
</PackageReleaseNotes>
    <RepositoryUrl>https://github.com/NeuCharFramework/NcfPackageSources</RepositoryUrl>
    <Configurations>Debug;Release;Test;TemplateRelease</Configurations>
  </PropertyGroup>
  <PropertyGroup Condition=" '$(Configuration)' == 'Release' ">
    <OutputPath>..\..\..\BuildOutPut</OutputPath>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="Senparc.CO2NET.WebApi" Version="1.5.2.1" />
    <!--<PackageReference Include="Microsoft.AspNetCore.Mvc.Versioning" Version="5.0.0" />-->
  </ItemGroup>
  <ItemGroup>
    <PackageReference Include="Microsoft.AspNetCore.Authentication.Cookies" Version="2.2.0" />
  </ItemGroup>
  <ItemGroup>
    <PackageReference Include="Senparc.Ncf.DatabasePlant" Version="0.13.6-beta1" />
  </ItemGroup>
  <ItemGroup>
    <PackageReference Include="Senparc.Ncf.XncfBase" Version="0.15.3.1-beta1+1218681265ff6390ef0242a77dad9ed99058eb16" />
    <PackageReference Include="Senparc.Ncf.AreaBase" Version="0.13.3.1-beta1+1218681265ff6390ef0242a77dad9ed99058eb16" />
    <None Include="App_Data\SenparcTraceLog\*" />
  </ItemGroup>
  <ItemGroup>
    <!-- 为解决 VS 2019 v16.8 的一个bug，编译遇到错误： error : rzc discover exited with code -2147450750，则需要添加下方引用 -->
    <!--<PackageReference Include="Microsoft.NET.Sdk.Razor" Version="3.1.10" />-->
  </ItemGroup>
  <ItemGroup>
    <None Include="..\..\..\icon.jpg" Pack="true" Visible="false" PackagePath="" />
  </ItemGroup>
  <ItemGroup>
    <Content Remove="App_Data\Database\SenparcConfig.config" />
  </ItemGroup>
  <ItemGroup>
    <None Include="App_Data\Database\SenparcConfig.config" />
  </ItemGroup>
</Project>